@prefix : <http://www.semanticweb.org/musical-instruments#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/musical-instruments> .

<http://www.semanticweb.org/musical-instruments> rdf:type owl:Ontology ;
                                                  <http://purl.org/dc/elements/1.1/creator> "Lorenzo Botto" ;
                                                  <http://purl.org/dc/elements/1.1/date> "27/01/2021" ;
                                                  <http://purl.org/dc/elements/1.1/description> "L'ontologia \"musical-instruments\" rappresenta l'intera conoscenza di un'enciclopedia per gli amanti della musica. Sono presenti gli strumenti musicali con i relativi artisti che li utilizzano, e le case produttrici degli strumenti musicali. Incorpora il vocabolario FOAF per gli artisti che suonano lo strumento musicale e l'ontologia \"mo\" (The Music Ontology) per rappresentare alcune classi, tra cui il genere musicale, gli strumenti musicali, il gruppo musicale, il solista e l'artista." ;
                                                  <http://purl.org/dc/elements/1.1/title> "Strumenti Musicali - Un'enciclopedia per gli amanti della musica" ;
                                                  <http://purl.org/vocab/vann/preferredNamespacePrefix> "musical-instruments" ;
                                                  <http://purl.org/vocab/vann/preferredNamespaceUri> "http://www.music.com/musical-instruments" ;
                                                  rdfs:label "musical-instruments" .

