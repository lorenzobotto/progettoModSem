<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty ;
                                                   rdfs:label "vann:preferredNamespaceUri" .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                                                               owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf> ;
                                                               rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ;
                                                               rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Item> ;
                                                               rdfs:comment "has item - The link to every item of the Bag"^^xsd:string ;
                                                               rdfs:label "has item"^^xsd:string .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> rdf:type owl:ObjectProperty ;
                                                                   rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Item> ;
                                                                   rdfs:range [ rdf:type owl:Class ;
                                                                                owl:complementOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Item>
                                                                              ] ;
                                                                   rdfs:comment "itemContent - The link to the actual resource to which the item refers."^^xsd:string ;
                                                                   rdfs:label "item content"^^xsd:string .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf> rdf:type owl:ObjectProperty ;
                                                              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                                                              rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Item> ;
                                                              rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ;
                                                              rdfs:comment "item of - The link from an item to the Bag where it is contained"^^xsd:string ;
                                                              rdfs:label "item of"^^xsd:string .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> rdf:type owl:ObjectProperty ;
                                                                              owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                                                                              rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                                              rdfs:range owl:Thing ;
                                                                              rdfs:comment "A relation between collections and entities, e.g. 'my collection of saxophones includes an old Adolphe Sax original alto' (i.e. my collection has member an Adolphe Sax alto)."@en ;
                                                                              rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                              rdfs:label "has member"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> rdf:type owl:ObjectProperty ;
                                                                               rdfs:domain owl:Thing ;
                                                                               rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                                               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                               rdfs:label "is member of"@en .


###  http://www.semanticweb.org/musical-instruments#bandSuonano
:bandSuonano rdf:type owl:ObjectProperty ;
             rdfs:domain :BandMusicale ;
             rdfs:range <http://purl.org/ontology/mo/Instrument> ;
             owl:propertyChainAxiom ( :haLavoratori
                                      :suona
                                    ) .


###  http://www.semanticweb.org/musical-instruments#compone
:compone rdf:type owl:ObjectProperty ;
         owl:inverseOf :compostoDa ;
         rdfs:domain :PezziBatteria ;
         rdfs:range :Batteria ;
         rdfs:comment "I pezzi della batteria che compongono la batteria (strumento musicale)"@it ;
         rdfs:label "compone"@it .


###  http://www.semanticweb.org/musical-instruments#compostoDa
:compostoDa rdf:type owl:ObjectProperty ;
            rdfs:domain :Batteria ;
            rdfs:range :PezziBatteria ;
            rdfs:comment "La batteria (strumento musicale) da che pezzi è composta"@it ;
            rdfs:label "compostoDa"@it .


###  http://www.semanticweb.org/musical-instruments#haLavoratori
:haLavoratori rdf:type owl:ObjectProperty ;
              owl:inverseOf :lavoraIn ;
              rdfs:domain :BandMusicale ;
              rdfs:range <http://purl.org/ontology/mo/MusicArtist> ;
              rdfs:comment "Una band musicale è rappresentata dagli artisti che suonano"@it ;
              rdfs:label "haLavoratori"@it .


###  http://www.semanticweb.org/musical-instruments#lavoraIn
:lavoraIn rdf:type owl:ObjectProperty ;
          rdfs:domain <http://purl.org/ontology/mo/MusicArtist> ;
          rdfs:range :BandMusicale ;
          rdfs:comment "Rappresenta gli artisti che suonano in una band musicale"@it ;
          rdfs:label "lavoraIn"@it .


###  http://www.semanticweb.org/musical-instruments#pezziProdottiDa
:pezziProdottiDa rdf:type owl:ObjectProperty ;
                 owl:inverseOf :producePezzi ;
                 rdfs:domain :PezziBatteria ;
                 rdfs:range :CasaProduttrice ;
                 rdfs:comment "I pezzi della batteria da quale casa produttrice sono prodotti"@it ;
                 rdfs:label "pezziProdottiDa"@it .


###  http://www.semanticweb.org/musical-instruments#prodottoDa
:prodottoDa rdf:type owl:ObjectProperty ;
            owl:inverseOf :produce ;
            rdfs:domain <http://purl.org/ontology/mo/Instrument> ;
            rdfs:range :CasaProduttrice ;
            rdfs:comment "Rappresenta lo strumento musicale da quale casa produttrice è prodotto"@it ;
            rdfs:label "prodottoDa"@it .


###  http://www.semanticweb.org/musical-instruments#produce
:produce rdf:type owl:ObjectProperty ;
         rdfs:domain :CasaProduttrice ;
         rdfs:range <http://purl.org/ontology/mo/Instrument> ;
         rdfs:comment "Rappresenta la casa produttrice che produce uno strumento musicale"@it ;
         rdfs:label "produce"@it .


###  http://www.semanticweb.org/musical-instruments#producePezzi
:producePezzi rdf:type owl:ObjectProperty ;
              rdfs:domain :CasaProduttrice ;
              rdfs:range :PezziBatteria ;
              rdfs:comment "Casa produttrice che produce pezzi per la batteria (strumento musicale)"@it ;
              rdfs:label "producePezzi"@it .


###  http://www.semanticweb.org/musical-instruments#suona
:suona rdf:type owl:ObjectProperty ;
       owl:inverseOf :suonatoDa ;
       rdfs:domain <http://purl.org/ontology/mo/MusicArtist> ;
       rdfs:range <http://purl.org/ontology/mo/Instrument> ;
       rdfs:comment "Rappresenta l'artista che suona lo strumento musicale"@it ;
       rdfs:label "suona"@it .


###  http://www.semanticweb.org/musical-instruments#suonatoCon
:suonatoCon rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain <http://purl.org/ontology/mo/Instrument> ;
            rdfs:range :Oggetto ;
            rdfs:comment "Rappresenta con quale ogetto è suonato lo strumento musicale"@it ;
            rdfs:label "suonatoCon"@it .


###  http://www.semanticweb.org/musical-instruments#suonatoDa
:suonatoDa rdf:type owl:ObjectProperty ;
           rdfs:domain <http://purl.org/ontology/mo/Instrument> ;
           rdfs:range <http://purl.org/ontology/mo/MusicArtist> ;
           rdfs:comment "Lo strumento musicale è rappresentato dagli artisti che lo suonano"@it ;
           rdfs:label "suonatoDa"@it .


###  http://www.semanticweb.org/musical-instruments#suonatoIn
:suonatoIn rdf:type owl:ObjectProperty ;
           rdfs:domain <http://purl.org/ontology/mo/Instrument> ;
           rdfs:range <http://purl.org/ontology/mo/Genre> ;
           rdfs:comment "Rappresenta in quale genere musicali si suonano gli strumenti musicali"@it ;
           rdfs:label "suonatoIn"@it .


###  http://www.semanticweb.org/musical-instruments#ubicataDa
:ubicataDa rdf:type owl:ObjectProperty ;
           owl:inverseOf :ubicataIn ;
           rdfs:domain :Citta ;
           rdfs:range :CasaProduttrice ;
           rdfs:comment "Rappresenta una città che è ubicata da una casa produttrice"@it ;
           rdfs:label "ubicataDa"@it .


###  http://www.semanticweb.org/musical-instruments#ubicataIn
:ubicataIn rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :CasaProduttrice ;
           rdfs:range :Citta ;
           rdfs:comment "Rappresenta una casa produttrice ubicata in una città"@it ;
           rdfs:label "ubicataIn"@it .


###  http://www.semanticweb.org/musical-instruments#utilizza
:utilizza rdf:type owl:ObjectProperty ;
          owl:inverseOf :utilizzatoDa ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain <http://purl.org/ontology/mo/MusicArtist> ;
          rdfs:range :Oggetto ;
          rdfs:comment "Rappresenta l'artiche che utilizza un oggetto per suonare uno strumento musicale"@it ;
          rdfs:label "utilizza"@it .


###  http://www.semanticweb.org/musical-instruments#utilizzatoDa
:utilizzatoDa rdf:type owl:ObjectProperty ;
              rdfs:domain :Oggetto ;
              rdfs:range <http://purl.org/ontology/mo/MusicArtist> ;
              rdfs:comment "Rappresenta un oggetto che è utilizzato da un'artista per suonare uno strumento musicale"@it ;
              rdfs:label "utilizzatoDa"@it .


###  http://xmlns.com/foaf/0.1/member
<http://xmlns.com/foaf/0.1/member> rdf:type owl:ObjectProperty ;
                                   rdfs:domain <http://purl.org/ontology/mo/MusicArtist> ;
                                   rdfs:range :BandMusicale ;
                                   rdfs:comment "member - Indicates a member of a Group"@en ;
                                   rdfs:label "(foaf) member"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/musical-instruments#Body
:Body rdf:type owl:DatatypeProperty ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :Basso
                                  :Chitarra
                                )
                  ] ;
      rdfs:range xsd:string ;
      rdfs:comment "Tipo del body della chitarra o del basso"@it ;
      rdfs:label "Body"@en .


###  http://www.semanticweb.org/musical-instruments#DescrizioneStrumento
:DescrizioneStrumento rdf:type owl:DatatypeProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( <http://purl.org/ontology/mo/Instrument>
                                                  :PezziBatteria
                                                )
                                  ] ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Descrizione dello strumento musicale"@it ;
                      rdfs:label "DescrizioneStrumento"@it .


###  http://www.semanticweb.org/musical-instruments#FondataNel
:FondataNel rdf:type owl:DatatypeProperty ;
            rdfs:domain :CasaProduttrice ;
            rdfs:range xsd:integer ;
            rdfs:comment "Anno di fondazione della casa produttrice"@it ;
            rdfs:label "FondataNel"@it .


###  http://www.semanticweb.org/musical-instruments#HaNumeroArtisti
:HaNumeroArtisti rdf:type owl:DatatypeProperty ;
                 rdfs:domain :BandMusicale ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "Rappresenta un dato di tipo intero per indicare il numero degli artisti"@it ;
                 rdfs:label "HaNumeroArtisti"@it .


###  http://www.semanticweb.org/musical-instruments#HaNumeroCorde
:HaNumeroCorde rdf:type owl:DatatypeProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Basso
                                           :Chitarra
                                         )
                           ] ;
               rdfs:range xsd:integer ;
               rdfs:comment "Numero delle corde di una chitarra"@it ;
               rdfs:label "HaNumeroCorde"@it .


###  http://www.semanticweb.org/musical-instruments#Immagine
:Immagine rdf:type owl:DatatypeProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( <http://purl.org/ontology/mo/Instrument>
                                      <http://purl.org/ontology/mo/MusicArtist>
                                      :BandMusicale
                                      :CasaProduttrice
                                      :PezziBatteria
                                    )
                      ] ;
          rdfs:range xsd:string ;
          rdfs:comment "Immagine di uno strumento musicale, artista, casa produttrice, pezzi batteria o band musicale"@it ;
          rdfs:label "Immagine"@it .


###  http://www.semanticweb.org/musical-instruments#Legni
:Legni rdf:type owl:DatatypeProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( <http://purl.org/ontology/mo/Instrument>
                                   :PezziBatteria
                                 )
                   ] ;
       rdfs:range xsd:string ;
       rdfs:comment "I legni utilizzati per la chitarra o un basso"@it ;
       rdfs:label "Legni"@it .


###  http://www.semanticweb.org/musical-instruments#NomeBandMusicale
:NomeBandMusicale rdf:type owl:DatatypeProperty ;
                  rdfs:domain :BandMusicale ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Nome della band musicale o solista"@it ;
                  rdfs:label "NomeBandMusicale"@it .


###  http://www.semanticweb.org/musical-instruments#NomeCasaProduttrice
:NomeCasaProduttrice rdf:type owl:DatatypeProperty ;
                     rdfs:domain :CasaProduttrice ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Nome della casa produttrice"@it ;
                     rdfs:label "NomeCasaProduttrice"@it .


###  http://www.semanticweb.org/musical-instruments#NomeCitta
:NomeCitta rdf:type owl:DatatypeProperty ;
           rdfs:domain :Citta ;
           rdfs:range xsd:string ;
           rdfs:comment "Nome della città"@it ;
           rdfs:label "NomeCitta"@it .


###  http://www.semanticweb.org/musical-instruments#NomeGenereMusicale
:NomeGenereMusicale rdf:type owl:DatatypeProperty ;
                    rdfs:domain <http://purl.org/ontology/mo/Genre> ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Nome del genere musicale"@it ;
                    rdfs:label "NomeGenereMusicale"@it .


###  http://www.semanticweb.org/musical-instruments#NomeStrumentoMusicale
:NomeStrumentoMusicale rdf:type owl:DatatypeProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( <http://purl.org/ontology/mo/Instrument>
                                                   :Fusto
                                                   :PezziBatteria
                                                   :Piatto
                                                 )
                                   ] ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Nome di uno strumento musicale"@it ;
                       rdfs:label "NomeStrumentoMusicale"@it .


###  http://www.semanticweb.org/musical-instruments#Ponte
:Ponte rdf:type owl:DatatypeProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Basso
                                   :Chitarra
                                 )
                   ] ;
       rdfs:range xsd:string ;
       rdfs:comment "Ponte della chitarra o del basso"@it ;
       rdfs:label "Ponte"@it .


###  http://xmlns.com/foaf/0.1/age
<http://xmlns.com/foaf/0.1/age> rdf:type owl:DatatypeProperty ;
                                rdfs:domain <http://purl.org/ontology/mo/MusicArtist> ;
                                rdfs:range xsd:nonNegativeInteger ;
                                rdfs:comment "Età dell'artista"@it ;
                                rdfs:label "(foaf) età"@it .


###  http://xmlns.com/foaf/0.1/birthday
<http://xmlns.com/foaf/0.1/birthday> rdf:type owl:DatatypeProperty ;
                                     rdfs:domain <http://purl.org/ontology/mo/MusicArtist> ;
                                     rdfs:range xsd:string ;
                                     rdfs:comment "Data di nascita dell'artista"@it ;
                                     rdfs:label "(foaf) data di nascita"@it .


###  http://xmlns.com/foaf/0.1/firstName
<http://xmlns.com/foaf/0.1/firstName> rdf:type owl:DatatypeProperty ;
                                      rdfs:domain <http://purl.org/ontology/mo/MusicArtist> ;
                                      rdfs:range xsd:string ;
                                      rdfs:comment "Nome dell'artista"@it ;
                                      rdfs:label "(foaf) nome"@it .


###  http://xmlns.com/foaf/0.1/gender
<http://xmlns.com/foaf/0.1/gender> rdf:type owl:DatatypeProperty ;
                                   rdfs:domain <http://purl.org/ontology/mo/MusicArtist> ;
                                   rdfs:range xsd:string ;
                                   rdfs:comment "Genere dell'artista"@it ;
                                   rdfs:label "(foaf) genere"@it .


###  http://xmlns.com/foaf/0.1/lastName
<http://xmlns.com/foaf/0.1/lastName> rdf:type owl:DatatypeProperty ;
                                     rdfs:domain <http://purl.org/ontology/mo/MusicArtist> ;
                                     rdfs:range xsd:string ;
                                     rdfs:comment "Cognome dell'artista"@it ;
                                     rdfs:label "(foaf) cognome"@it .


#################################################################
#    Classes
#################################################################

###  http://purl.org/ontology/mo/Genre
<http://purl.org/ontology/mo/Genre> rdf:type owl:Class ;
                                    owl:equivalentClass [ rdf:type owl:Class ;
                                                          owl:oneOf ( :Blues
                                                                      :ProgressiveMetal
                                                                      :Rock
                                                                    )
                                                        ] ;
                                    rdfs:comment "I generi musicali della musica"@it ;
                                    rdfs:label "GenereMusicale"@it .


###  http://purl.org/ontology/mo/Instrument
<http://purl.org/ontology/mo/Instrument> rdf:type owl:Class ;
                                         owl:equivalentClass [ rdf:type owl:Restriction ;
                                                               owl:onProperty :suonatoIn ;
                                                               owl:someValuesFrom <http://purl.org/ontology/mo/Genre>
                                                             ] ;
                                         rdfs:comment "Un'entità per descrivere gli strumenti musicali."@it ;
                                         rdfs:label "StrumentoMusicale"@it .


###  http://purl.org/ontology/mo/MusicArtist
<http://purl.org/ontology/mo/MusicArtist> rdf:type owl:Class ;
                                          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty :lavoraIn ;
                                                                                       owl:someValuesFrom :BandMusicale
                                                                                     ]
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty :suona ;
                                                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                       owl:onClass <http://purl.org/ontology/mo/Instrument>
                                                                                     ]
                                                                                   ) ;
                                                                rdf:type owl:Class
                                                              ] ;
                                          rdfs:comment "Artista in campo musicale"@it ;
                                          rdfs:label "Artista"@it .


###  http://purl.org/ontology/mo/MusicGroup
<http://purl.org/ontology/mo/MusicGroup> rdf:type owl:Class ;
                                         owl:equivalentClass [ rdf:type owl:Restriction ;
                                                               owl:onProperty :HaNumeroArtisti ;
                                                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                    owl:onDatatype xsd:integer ;
                                                                                    owl:withRestrictions ( [ xsd:minExclusive 1
                                                                                                           ]
                                                                                                         )
                                                                                  ]
                                                             ] ;
                                         rdfs:subClassOf :BandMusicale ;
                                         rdfs:comment "Band musicale formata da almeno 2 artisti"@it ;
                                         rdfs:label "Gruppo"@it .


###  http://purl.org/ontology/mo/SoloMusicArtist
<http://purl.org/ontology/mo/SoloMusicArtist> rdf:type owl:Class ;
                                              owl:equivalentClass [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :HaNumeroArtisti ;
                                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                    owl:onDataRange xsd:integer
                                                                  ] ;
                                              rdfs:subClassOf :BandMusicale ;
                                              rdfs:comment "Band musicale formata da un solo artista"@it ;
                                              rdfs:label "Solista"@it .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                           rdfs:comment "Bag - Collection that can have a number of copies of each object"^^xsd:string ;
                                                           rdfs:label "(collections) Bag"^^xsd:string .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> rdf:type owl:Class ;
                                                                               owl:disjointWith <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Item> ;
                                                                               rdfs:comment "Any container for entities that share one or more common properties. E.g. \"stone objects\", \"the nurses\", \"the Louvre Aegyptian collection\". A collection is not a logical class: a collection is a first-order entity, while a class is a second-order one." ;
                                                                               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
